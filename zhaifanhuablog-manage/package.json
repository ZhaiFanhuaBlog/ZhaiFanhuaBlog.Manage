{
  "name": "zhaifanhuablog-manage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "bootstrap": "yarn install",
    "serve": "npx --max_old_space_size=4096 vite",
    "dev": "npx --max_old_space_size=4096 vite",
    "pro": "vite build --mode production",
    "build": "vue-tsc --noEmit && vite build",
    "build:no-cache": "yarn clean:cache && npm run build",
    "report": "yarn -v",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "yarn -v",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,build,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "test:unit": "jest",
    "test:gzip": "yarn -v",
    "test:br": "yarn -v",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "install:husky": "yarn -v",
    "gen:icon": "yarn -v"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "pinia": "^2.0.18",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-define-config": "^1.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "naive-ui": "^2.32.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.3",
    "vfonts": "^0.0.3",
    "vite": "^3.0.0",
    "vue-tsc": "^0.38.4"
  }
}
